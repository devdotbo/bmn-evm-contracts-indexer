      "pg": "/usr/bin",
      "ppas": ""
    },
    "query_tool:auto_commit": false,
    "query_tool:auto_rollback_on_error": true,
    "query_tool:connection_status": true,
    "query_tool:connection_status_fetch_time": 5,
    "query_tool:display_connection_status": true,
    "query_tool:font_size": "13",
    "results_grid:thousands_separator": ",",
    "sqleditor:query_tool_toggle": "alt+q"
